{
  "$schema": "../.sitecore/schemas/ModuleFile.schema.json",
  "namespace": "XDF.MyModule",
  "items": {
    "includes": [
      {
        "name": "mymodule-templates",
        "path": "/sitecore/templates/Feature/MyModule",
        "allowedPushOperations": "CreateUpdateAndDelete"
      },
      {
        "name": "mymodule-branches",
        "path": "/sitecore/templates/Branches/Feature/MyModule",
        "allowedPushOperations": "CreateUpdateAndDelete"
      },
      {
        "name": "mymodule-project-placeholders",
        "path": "/sitecore/layout/Placeholder Settings/Feature/MyModule",
        "allowedPushOperations": "CreateUpdateAndDelete"
      },
      {
        "name": "mymodule-renderings",
        "path": "/sitecore/layout/Renderings/Feature/MyModule",
        "allowedPushOperations": "CreateUpdateAndDelete"
      }
    ]
  }
}
